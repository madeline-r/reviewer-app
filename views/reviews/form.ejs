<div class="form-group">
    <label for="title">Title</label>
    <input required value="<%= review.title %>" type="text" name="title" id="title" class="form-control mb-3">
</div>
<div class="form-group">
    <label for="text">Summary</label>
    <textarea name="summary" id="summary" class="form-control mb-3"> <%= review.summary %></textarea>
</div>
<div class="form-group">
    <label for="text">Review</label>
    <textarea required name="review" id="review" class="form-control mb-3"><%= review.review %></textarea>
</div>
<button type="submit" class="btn btn-outline-primary">Upload</button>
<a href="/" class="btn btn-outline-secondary">Cancel</a>

